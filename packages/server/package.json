  "author": {
    "name": "Safis",
    "email": "team@safis.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/safis-io/safis-cms/issues"
  },
  "homepage": "https://github.com/safis-io/safis-cms#readme",
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.14.13"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@graphql-toolkit/schema-merging": "^0.10.6",
    "core-js": "^3.6.5",
    "dotenv": "^10.0.0",
